t = int(input())
for _ in range(t):
    n = int(input())
    s = input().strip()
    
    freq = {}
    possible = False
    
    for ch in s:
        freq[ch] = freq.get(ch, 0) + 1
        if freq[ch] >= 2:
            possible = True
            break
    
    if possible:
        print("Yes")
    else:
        print("No")
