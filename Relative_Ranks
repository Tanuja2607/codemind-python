from typing import List

class Solution:
    def findRelativeRanks(self, score: List[int]) -> List[str]:
        n = len(score)
        max_score = max(score)
        bucket = [None] * (max_score + 1)
        for i, s in enumerate(score):
            bucket[s] = i

        res = [""] * n
        rank = 1
        for s in range(max_score, -1, -1):
            if bucket[s] is not None:
                idx = bucket[s]
                if rank == 1:
                    res[idx] = "Gold Medal"
                elif rank == 2:
                    res[idx] = "Silver Medal"
                elif rank == 3:
                    res[idx] = "Bronze Medal"
                else:
                    res[idx] = str(rank)
                rank += 1
        return res
