def print_formatted(number):
    # Find the width of the binary representation of the maximum number
    width = len(bin(number)[2:])

    # Loop from 1 to number
    for i in range(1, number + 1):
        # Print decimal, octal, hexadecimal (uppercase), binary with proper spacing
        print(
            str(i).rjust(width),
            oct(i)[2:].rjust(width),
            hex(i)[2:].upper().rjust(width),
            bin(i)[2:].rjust(width)
        )

if __name__ == '__main__':
    n = int(input())
    print_formatted(n)
