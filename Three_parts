n=int(input())
arr=list(map(int,input().split()))
total_sum=0
for i in range(n):
    total_sum+=arr[i]
if total_sum%3!=0:
    print("false")
else:
    target=total_sum//3
    curr_sum=0
    count=0
    for i in range(n):
        curr_sum+=arr[i]
        if curr_sum==target:
            count+=1
            curr_sum=0
    if count>=3:
        print("true")
    else:
        print("false")       
