class Solution:
    def subarraySum(self, arr, target):
      l = 0
      s = 0
      
      for r in range(len(arr)):
          s += arr[r]
          
          while s>target and l<=r:
              s -= arr[l]
              l +=1
          if s == target:
              return [l+1, r+1]
              
      return [-1] 
